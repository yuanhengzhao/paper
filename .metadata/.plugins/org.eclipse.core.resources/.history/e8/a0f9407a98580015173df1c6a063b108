require_relative '../../spec_helper'

describe ProntoForms::EscrubKopParser do

  let(:missing_image) { "/assets/missing.jpg" }

  describe "max_version" do
    subject { ProntoForms::EscrubKopParser.new }
    it "should not throw an error" do
      lambda { subject.max_version }.should_not raise_error
    end
  end

  describe "parse" do

  # call the parse method on a parameter defined in the sub-describes
    subject { ProntoForms::EscrubKopParser.new.parse(form_hash) }

    # ensure that form_hash has been properly set for the specs
    before(:each) do
      form_hash.should_not be_nil
    end

    describe "for a hash missing pages" do

      let(:form_hash) {
        f = Rails.root.join('spec', 'resources', 'forms', 'escrub_kop', 'missing_pages.json')
        return ActiveSupport::JSON.decode(f.read)
      }

      it "should throw a parser error" do
        lambda { subject }.should raise_error(ParserException, "Could not find expected page [page: 1]")
      end

    end
    
    describe "for a valid hash" do

      let(:form_hash) {
        f = Rails.root.join('spec', 'resources', 'forms', 'escrub_kop', 'valid_inspection_v2.json')
        return ActiveSupport::JSON.decode(f.read)
      }

      describe "for valid input" do
        let(:expected) {
          o = Forms::EscrubKop.new

          o.reference_number = "20150505175908-1822071394"
          o.server_receive_date = "2015-05-05T13:59:11-04:00"
          o.device_submit_date = "2015-05-05T13:58:43-04:00"
          o.username = "mliao@robinsonsolutions.com"
          o.version = "2"
          o.altitude = 0.0,
          o.latitude = 43.652163100000003
          o.longitude = -79.382149499999997
          o.source = "prontoforms"

          o.date = "2015-05-05"
          o.time = "13:22:00-04:00"
          o.crew = "A1"
          o.shift = "Day"
          o.check_list = "Module Inspection"
          
          
          o.bc_matrix_dist = "Good"
          o.bc_matrix_flow = "OK"
          o.bc_particles_trough = "No"
          o.bc_residues = "No"
          o.bc_remarks = "1"
          bc_picture1 = { "bytes" => "",
            "contentType" => "image/jpeg",
            "filename" => "photo_2015-05-05_13-57-58-377.jpeg",
            "identifier" => "269818110433"
          }
          bc_picture2 = { "bytes" => "",
            "contentType" => "image/jpeg",
            "filename" => "photo_2015-05-05_13-58-28-800.jpeg",
            "identifier" => "269818110439"
          }
          bc_picture3 = { "bytes" => "",
            "contentType" => "image/jpeg",
            "filename" => "photo_2015-05-05_13-58-28-800.jpeg",
            "identifier" => "269818110439"
          }
          bc_picture4 = { "bytes" => "",
            "contentType" => "image/jpeg",
            "filename" => "photo_2015-05-05_13-58-28-800.jpeg",
            "identifier" => "269818110439"
          }
          
          o.bc_picture1 = ProntoForms::SourceParser.create_attachment(bc_picture1)
          o.bc_picture2 = ProntoForms::SourceParser.create_attachment(bc_picture2)
          o.bc_picture3 = ProntoForms::SourceParser.create_attachment(bc_picture3)
          o.bc_picture4 = ProntoForms::SourceParser.create_attachment(bc_picture4)

          o.cc_matrix_dist = "Bad"
          o.cc_matrix_flow = "No"
          o.cc_particles_trough = "Yes"
          o.cc_residues = "Yes"
          o.cc_remarks = "2"
          cc_picture1 = { "bytes" => "",
            "contentType" => "image/jpeg",
            "filename" => "photo_2015-05-05_13-58-28-800.jpeg",
            "identifier" => "269818110439"
          }
          cc_picture2 = { "bytes" => "",
            "contentType" => "image/jpeg",
            "filename" => "photo_2015-05-05_13-57-58-377.jpeg",
            "identifier" => "269818110433"
          }
          cc_picture3 = { "bytes" => "",
            "contentType" => "image/jpeg",
            "filename" => "photo_2015-05-05_13-57-58-377.jpeg",
            "identifier" => "269818110433"
          }
          cc_picture4 = { "bytes" => "",
            "contentType" => "image/jpeg",
            "filename" => "photo_2015-05-05_13-57-58-377.jpeg",
            "identifier" => "269818110433"
          }
          cc_picture5 = { "bytes" => "",
            "contentType" => "image/jpeg",
            "filename" => "photo_2015-05-05_13-57-58-377.jpeg",
            "identifier" => "269818110433"
          }
          o.cc_picture1 = ProntoForms::SourceParser.create_attachment(cc_picture1)
          o.cc_picture2 = ProntoForms::SourceParser.create_attachment(cc_picture2)
          o.cc_picture3 = ProntoForms::SourceParser.create_attachment(cc_picture3)
          o.cc_picture4 = ProntoForms::SourceParser.create_attachment(cc_picture4)
          o.cc_picture5 = ProntoForms::SourceParser.create_attachment(cc_picture5)

          return o
        }

        subject {
          o = ProntoForms::EscrubKopParser.new.parse(form_hash)
          #puts o.to_json
          return o
        }

        it "should return a Forms::EscrubKop object" do
          subject.should be_a_kind_of(Forms::EscrubKop)
        end
        
        it "should be valid" do
          expect(subject).to  be_valid
          expect(expected).to be_valid
        end
        
        describe "should match expected" do

          before(:each) do
            subject.save
            subject.reload
          end

          it "should have the same base form values" do
            expect(subject).to have_same_base_form_attributes_as(expected)
          end

          its("date") { should eq(expected.date) }
          its("time") { should eq(expected.time) }
          its("crew") { should eq(expected.crew) }
          its("shift") { should eq(expected.shift) }
          its("check_list") { should eq(expected.check_list) }
          
          its("bc_matrix_dist") { should eq(expected.bc_matrix_dist) }
          its("bc_matrix_flow") { should eq(expected.bc_matrix_flow) }
          its("bc_particles_trough") { should eq(expected.bc_particles_trough) }
          its("bc_residues") { should eq(expected.bc_residues) }
          its("bc_remarks") { should eq(expected.bc_remarks) }
          
          its("bc_picture1") { should_not be_nil }
          its("bc_picture1") { should_not eq(missing_image) }
          its("bc_picture1_content_type") { should eq(expected.bc_picture1_content_type) }
          its("bc_picture1_file_name") { should eq(expected.bc_picture1_file_name) }
          its("bc_picture1_file_size") { should eq(expected.bc_picture1_file_size) }
          
          its("bc_picture2") { should_not be_nil }
          its("bc_picture2") { should_not eq(missing_image) }
          its("bc_picture2_content_type") { should eq(expected.bc_picture2_content_type) }
          its("bc_picture2_file_name") { should eq(expected.bc_picture2_file_name) }
          its("bc_picture2_file_size") { should eq(expected.bc_picture2_file_size) }
          
          its("bc_picture3") { should_not be_nil }
          its("bc_picture3") { should_not eq(missing_image) }
          its("bc_picture3_content_type") { should eq(expected.bc_picture3_content_type) }
          its("bc_picture3_file_name") { should eq(expected.bc_picture3_file_name) }
          its("bc_picture3_file_size") { should eq(expected.bc_picture3_file_size) }
          
          its("bc_picture4") { should_not be_nil }
          its("bc_picture4") { should_not eq(missing_image) }
          its("bc_picture4_content_type") { should eq(expected.bc_picture4_content_type) }
          its("bc_picture4_file_name") { should eq(expected.bc_picture4_file_name) }
          its("bc_picture4_file_size") { should eq(expected.bc_picture4_file_size) }
          
          
          its("cc_matrix_dist") { should eq(expected.cc_matrix_dist) }
          its("cc_matrix_flow") { should eq(expected.cc_matrix_flow) }
          its("cc_particles_trough") { should eq(expected.cc_particles_trough) }
          its("cc_residues") { should eq(expected.cc_residues) }
          its("cc_remarks") { should eq(expected.cc_remarks) }
          its("cc_picture") { should_not be_nil }
          its("cc_picture") { should_not eq(missing_image) }
          its("cc_picture_content_type") { should eq(expected.cc_picture_content_type) }
          its("cc_picture_file_name") { should eq(expected.cc_picture_file_name) }
          its("cc_picture_file_size") { should eq(expected.cc_picture_file_size) }

          its("cc_picture1") { should_not be_nil }
          its("cc_picture1") { should_not eq(missing_image) }
          its("cc_picture1_content_type") { should eq(expected.cc_picture1_content_type) }
          its("cc_picture1_file_name") { should eq(expected.cc_picture1_file_name) }
          its("cc_picture1_file_size") { should eq(expected.cc_picture1_file_size) }

          its("cc_picture2") { should_not be_nil }
          its("cc_picture2") { should_not eq(missing_image) }
          its("cc_picture2_content_type") { should eq(expected.cc_picture2_content_type) }
          its("cc_picture2_file_name") { should eq(expected.cc_picture2_file_name) }
          its("cc_picture2_file_size") { should eq(expected.cc_picture2_file_size) }
          
          its("cc_picture3") { should_not be_nil }
          its("cc_picture3") { should_not eq(missing_image) }
          its("cc_picture3_content_type") { should eq(expected.cc_picture3_content_type) }
          its("cc_picture3_file_name") { should eq(expected.cc_picture3_file_name) }
          its("cc_picture3_file_size") { should eq(expected.cc_picture3_file_size) }
          
          its("cc_picture4") { should_not be_nil }
          its("cc_picture4") { should_not eq(missing_image) }
          its("cc_picture4_content_type") { should eq(expected.cc_picture4_content_type) }
          its("cc_picture4_file_name") { should eq(expected.cc_picture4_file_name) }
          its("cc_picture4_file_size") { should eq(expected.cc_picture4_file_size) }
          
          its("cc_picture5") { should_not be_nil }
          its("cc_picture5") { should_not eq(missing_image) }
          its("cc_picture5_content_type") { should eq(expected.cc_picture5_content_type) }
          its("cc_picture5_file_name") { should eq(expected.cc_picture5_file_name) }
          its("cc_picture5_file_size") { should eq(expected.cc_picture5_file_size) }
        end
      end
    end
  end
end