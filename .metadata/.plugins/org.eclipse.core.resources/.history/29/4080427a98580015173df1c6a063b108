require_relative '../../spec_helper'

describe ProntoForms::TaskInspectionParser do

  let(:missing_image) { "/assets/missing.jpg" }

  describe "max_version" do
    subject { ProntoForms::TaskInspectionParser.new }
    it "should not throw an error" do
      lambda { subject.max_version }.should_not raise_error
    end
  end

  describe "parse" do

  # call the parse method on a parameter defined in the sub-describes
    subject { ProntoForms::TaskInspectionParser.new.parse(form_hash) }

    # ensure that form_hash has been properly set for the specs
    before(:each) do
      form_hash.should_not be_nil
    end

    describe "for a hash missing pages" do

      let(:form_hash) {
        f = Rails.root.join('spec', 'resources', 'forms', 'task_inspection', 'missing_pages.json')
        return ActiveSupport::JSON.decode(f.read)
      }

      it "should throw a parser error" do
        lambda { subject }.should raise_error(ParserException, "Could not find expected answer [page: 0][answer: 4]")
      end

    end

    describe "for a hash with too many pictures" do

      let(:form_hash) {
        f = Rails.root.join('spec', 'resources', 'forms', 'task_inspection', 'too_many_pictures.json')
        return ActiveSupport::JSON.decode(f.read)
      }

      it "should throw a parser error" do
        lambda { subject }.should raise_error(ParserException, "Too many images attached to form")
      end

    end

    describe "for a hash with zero tasks" do

      let(:form_hash) {
        f = Rails.root.join('spec', 'resources', 'forms', 'task_inspection', '0_tasks.json')
        return ActiveSupport::JSON.decode(f.read)
      }

      it "should throw a parser error" do
        lambda { subject }.should raise_error(ParserException, "At least one task is required")
      end

    end

    describe "for a hash with too many tasks" do

      let(:form_hash) {
        f = Rails.root.join('spec', 'resources', 'forms', 'task_inspection', 'too_many_tasks.json')
        return ActiveSupport::JSON.decode(f.read)
      }

      it "should throw a parser error" do
        lambda { subject }.should raise_error(ParserException, "Too many tasks submitted")
      end

    end

    describe "for a valid hash" do

      let(:form_hash) {
        f = Rails.root.join('spec', 'resources', 'forms', 'task_inspection', filename)
        return ActiveSupport::JSON.decode(f.read)
      }

      describe "for 1 task" do

        let(:filename) { "1_task.json" }

        let(:expected) {
          Forms::TaskInspection.new({
            :reference_number => "20140828185337-1815045900",
            :server_receive_date => "2014-08-28T14:53:43-04:00",
            :device_submit_date => "2014-08-28T14:53:29-04:00",
            :username => "mliao@robinsonsolutions.com",
            :version => "1",
            :altitude => nil,
            :latitude => nil,
            :longitude => nil,
            :source => "prontoforms",

            :inspection_date => "2014-08-28",
            :client => "BMW",
            :site => "Spartanburg",
            :shop => "North Paint",
            :num_tasks => 1,

            :task1 => "t3",
            :abnormality1 => "No",
            :abnormality_desc1 => "ad",

            :abnormality_picture1 => ProntoForms::SourceParser.create_attachment({
              "filename" => "images.jpg",
              "contentType" => "image/jpeg",
              "bytes" => ""
            }),

            :picture_before_task1 => ProntoForms::SourceParser.create_attachment({
              "filename" => "images.jpg",
              "contentType" => "image/jpeg",
              "bytes" => ""
            }),

            :picture_after_task1 => ProntoForms::SourceParser.create_attachment({
              "filename" => "images.jpg",
              "contentType" => "image/jpeg",
              "bytes" => ""
            }),

            :quality_evaluation1 => "5",
            :comments_task1 => "hello",

            :task2 => nil,
            :abnormality2 => nil,
            :abnormality_desc2 => nil,

            :abnormality_picture2 => nil,

            :picture_before_task2 => nil,

            :picture_after_task2 => nil,

            :quality_evaluation2 => nil,
            :comments_task2 => nil,

            :task3 => nil,
            :abnormality3 => nil,
            :abnormality_desc3 => nil,

            :abnormality_picture3 => nil,

            :picture_before_task3 => nil,

            :picture_after_task3 => nil,

            :quality_evaluation3 => nil,
            :comments_task3 => nil
          })
        }

        it "should return a Forms::TaskInspection object" do
          subject.should be_a_kind_of(Forms::TaskInspection)
        end
        
        it "should be valid" do
          expect(subject).to  be_valid
          expect(expected).to be_valid
        end

        describe "should match expected" do

          before(:each) do
            subject.save
            subject.reload
          end
          
          it "should have the same base form values" do
            expect(subject).to have_same_base_form_attributes_as(expected)
          end

          specify { subject.should have_same_task_inspection_attributes_as(expected) }
        end

      end

      describe "for 2 tasks" do

        let(:filename) { "2_tasks.json" }

        let(:expected) {
          Forms::TaskInspection.new({
            :reference_number => "20140828185337-1815045900",
            :server_receive_date => "2014-08-28T14:53:43-04:00",
            :device_submit_date => "2014-08-28T14:53:29-04:00",
            :username => "mliao@robinsonsolutions.com",
            :version => "1",
            :altitude => nil,
            :latitude => nil,
            :longitude => nil,
            :source => "prontoforms",

            :inspection_date => "2014-08-28",
            :client => "BMW",
            :site => "Spartanburg",
            :shop => "North Paint",
            :num_tasks => 2,

            :task1 => "t2",
            :abnormality1 => "Yes",
            :abnormality_desc1 => "ab",

            :abnormality_picture1 => ProntoForms::SourceParser.create_attachment({
              "filename" => "images.jpg",
              "contentType" => "image/jpeg",
              "bytes" => ""
            }),

            :picture_before_task1 => ProntoForms::SourceParser.create_attachment({
              "filename" => "images.jpg",
              "contentType" => "image/jpeg",
              "bytes" => ""
            }),

            :picture_after_task1 => ProntoForms::SourceParser.create_attachment({
              "filename" => "images.jpg",
              "contentType" => "image/jpeg",
              "bytes" => ""
            }),

            :quality_evaluation1 => "5",
            :comments_task1 => "ct",

            :task2 => "t3",
            :abnormality2 => "No",
            :abnormality_desc2 => "ad",

            :abnormality_picture2 => ProntoForms::SourceParser.create_attachment({
              "filename" => "images.jpg",
              "contentType" => "image/jpeg",
              "bytes" => ""
            }),

            :picture_before_task2 => ProntoForms::SourceParser.create_attachment({
              "filename" => "images.jpg",
              "contentType" => "image/jpeg",
              "bytes" => ""
            }),

            :picture_after_task2 => ProntoForms::SourceParser.create_attachment({
              "filename" => "images.jpg",
              "contentType" => "image/jpeg",
              "bytes" => ""
            }),

            :quality_evaluation2 => "5",
            :comments_task2 => "hello",

            :task3 => nil,
            :abnormality3 => nil,
            :abnormality_desc3 => nil,

            :abnormality_picture3 => nil,

            :picture_before_task3 => nil,

            :picture_after_task3 => nil,

            :quality_evaluation3 => nil,
            :comments_task3 => nil
          })
        }

        it "should return a Forms::TaskInspection object" do
          subject.should be_a_kind_of(Forms::TaskInspection)
        end

        it "should be valid" do
          expect(subject).to  be_valid
          expect(expected).to be_valid
        end

        describe "should match expected" do

          before(:each) do
            subject.save
            subject.reload
          end
          
          it "should have the same base form values" do
            expect(subject).to have_same_base_form_attributes_as(expected)
          end

          specify { subject.should have_same_task_inspection_attributes_as(expected) }
        end

      end

      describe "for 3 tasks" do

        let(:filename) { "3_tasks.json" }

        let(:expected) {
          Forms::TaskInspection.new({
            :reference_number => "20140828185337-1815045900",
            :server_receive_date => "2014-08-28T14:53:43-04:00",
            :device_submit_date => "2014-08-28T14:53:29-04:00",
            :username => "mliao@robinsonsolutions.com",
            :version => "1",
            :altitude => nil,
            :latitude => nil,
            :longitude => nil,
            :source => "prontoforms",

            :inspection_date => "2014-08-28",
            :client => "BMW",
            :site => "Spartanburg",
            :shop => "North Paint",
            :num_tasks => 3,

            :task3 => "t3",
            :abnormality3 => "No",
            :abnormality_desc3 => "ad",

            :abnormality_picture3 => ProntoForms::SourceParser.create_attachment({
              "filename" => "images.jpg",
              "contentType" => "image/jpeg",
              "bytes" => ""
            }),

            :picture_before_task3 => ProntoForms::SourceParser.create_attachment({
              "filename" => "images.jpg",
              "contentType" => "image/jpeg",
              "bytes" => ""
            }),

            :picture_after_task3 => ProntoForms::SourceParser.create_attachment({
              "filename" => "images.jpg",
              "contentType" => "image/jpeg",
              "bytes" => ""
            }),

            :quality_evaluation3 => "5",
            :comments_task3 => "hello",

            :task2 => "t2",
            :abnormality2 => "Yes",
            :abnormality_desc2 => "ab",

            :abnormality_picture2 => ProntoForms::SourceParser.create_attachment({
              "filename" => "images.jpg",
              "contentType" => "image/jpeg",
              "bytes" => ""
            }),

            :picture_before_task2 => ProntoForms::SourceParser.create_attachment({
              "filename" => "images.jpg",
              "contentType" => "image/jpeg",
              "bytes" => ""
            }),

            :picture_after_task2 => ProntoForms::SourceParser.create_attachment({
              "filename" => "images.jpg",
              "contentType" => "image/jpeg",
              "bytes" => ""
            }),

            :quality_evaluation2 => "5",
            :comments_task2 => "ct",

            :task1 => "t1",
            :abnormality1 => "Yes",
            :abnormality_desc1 => "ab",

            :abnormality_picture1 => ProntoForms::SourceParser.create_attachment({
              "filename" => "images.jpg",
              "contentType" => "image/jpeg",
              "bytes" => ""
            }),

            :picture_before_task1 => ProntoForms::SourceParser.create_attachment({
              "filename" => "images.jpg",
              "contentType" => "image/jpeg",
              "bytes" => ""
            }),

            :picture_after_task1 => ProntoForms::SourceParser.create_attachment({
              "filename" => "images.jpg",
              "contentType" => "image/jpeg",
              "bytes" => ""
            }),

            :quality_evaluation1 => "5",
            :comments_task1 => "ct"
          })
        }

        it "should return a Forms::TaskInspection object" do
          subject.should be_a_kind_of(Forms::TaskInspection)
        end

        it "should be valid" do
          expect(subject).to  be_valid
          expect(expected).to be_valid
        end

        describe "should match expected" do

          before(:each) do
            subject.save
            subject.reload
          end
          
          it "should have the same base form values" do
            expect(subject).to have_same_base_form_attributes_as(expected)
          end

          specify { subject.should have_same_task_inspection_attributes_as(expected) }
        end

      end

    end

  end

end